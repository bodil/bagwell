{
  "compilerOptions": {
    "sourceMap": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "allowJs": false,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "removeComments": false,
    "jsx": "preserve",
    "lib": [
      "es6"
    ]
  }
}
